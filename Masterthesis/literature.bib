@article{Meier2016,
abstract = {We investigate the Josephson critical current {\$}I{\_}c(\backslashPhi){\$} of a wide superconductor-normal metal-superconductor (SNS) junction as a function of the magnetic flux {\$}\backslashPhi{\$} threading it. Electronic trajectories reflected from the side edges alter the function {\$}I{\_}c(\backslashPhi){\$} as compared to the conventional Fraunhofer-type dependence. At weak magnetic fields, {\$}B\backslashlesssim \backslashPhi{\_}0/d{\^{}}2{\$}, the edge effect lifts zeros in {\$}I{\_}c(\backslashPhi){\$} and gradually shifts the minima of that function toward half-integer multiples of the flux quantum. At {\$}B{\textgreater}\backslashPhi{\_}0/d{\^{}}2{\$}, the edge effect leads to an accelerated decay of the critical current {\$}I{\_}c(\backslashPhi){\$} with increasing {\$}\backslashPhi{\$}. At larger fields, eventually, the system is expected to cross into a regime of "classical" mesoscopic fluctuations that is specific for wide ballistic SNS junctions with rough edges.},
archivePrefix = {arXiv},
