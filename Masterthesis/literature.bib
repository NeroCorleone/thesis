journal = {Physical Review B},
number = {18},
pages = {1--13},
title = {{Edge effects in the magnetic interference pattern of a ballistic SNS junction}},
volume = {93},
year = {2016}
}
@article{Svidzinsky1973,
abstract = {In our previous work 1 we gave a simple method of finding Green functions for an SNS sandwich. The usual model was considered where the effective masses and Fermi velocities are the same both in the superconductors and in the normal metals, the only differences lying in the values of the ef-fective interaction constants that are zero in the normal and negative in a superconductive metal. The magnetic field produced by the current was ignored. Finally, the ordering parameter modulus was assumed equal in all points of a superconductor up to the boundary. These assumptions being made, the problem of finding Green functions can be solved exactly, and afterward one may calculate the supercurrent through the junction. This problem has been considered by Kulik 2 ; his results, however, differ from ours. After completion of Ref. 1 we learned about Ishii's paper 3 and quite recently about Bardeen and Johnson's article. 4 In Ref. 3 a solution (by a very awkward method) for T = 0 K is given that agrees with ours, and the source of discrepancy in the calculations by Kulik and Ishii is discussed by Bardeen and Johnson, but their consideration is approximate and gives no exhaustive answer. The aim of the present note is to clarify the discrepancy between the results of Ref. 2 and Refs. I and 3. By means of the method used in Ref. 2 we obtain the result of Ref. 1. It will be seen that this result appears due to canceling discrete and continuous spectra states contributing to the current, so the difference of relatively large terms becomes small. The canceling should be expected from the general theorem of Heisenberg (see e.g., Refs. 5 and 6) ; its validity in our problem can be verified by direct calculations. Using an approximate method one clearly ought to remain within the same ranges of accuracy while calculating both contributions. Otherwise, large terms may remain after summing. It did happen in Ref. 2. Direct the z axis normally to the boundary between the normal and superconductive parts of the sandwich. The origin is placed in the middle of the normal layer, the width of which is denoted by d. Instead of the Gor'kov equations we shall write the equations for Green functions in a t representa-tion, 7 i.e., use the lowered order equations, in which it is taken into account that the essential momenta are near to the Fermi momentum Po. These equations are lie) + i(d/dt)]G{\~{}}+)(x, t, t') + A(tvox)Fo,(x , t, t') = 6(t -t')},
author = {Svidzinsky, A. V. and Antsygina, T. N. and Bratus', E. N.},
doi = {10.1007/BF00655245},
file = {:home/nefta/Dropbox/master/literature/antsygina{\_}bratus{\_}svidzinskii.pdf:pdf},
issn = {00222291},
journal = {Journal of Low Temperature Physics},
number = {1-2},
pages = {131--136},
title = {{Concerning the theory of the Josephson effect in pure SNS junctions}},
volume = {10},
year = {1973}
}
